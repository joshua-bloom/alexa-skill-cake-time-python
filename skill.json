    "publishingInformation": {
      "locales": {
        "en-US": {
          "summary": "This app allows users to hear a countdown to their birthday",
          "examplePhrases": [
            "Alexa open happy birthday cake time",
            "I was born on January first two thousand and one"
          ],
          "keywords": [
            "happy",
            "birthday",
            "cake",
            "time",
            "countdown",
            "timer"
          ],
          "name": "Happy Birthday Cake Time",
          "description": "Alexa asks for your birthday and remembers it. The next time you open the skill, Alexa tells you how many days there are until your next birthday. When your special day arrives, Alexa will wish you a Happy Birthday!",
